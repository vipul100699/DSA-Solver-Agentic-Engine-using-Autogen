def bubble_sort(arr):
    n = len(arr)
    for i in range(n):
        for j in range(0, n - i - 1):
            if arr[j] > arr[j + 1]:
                arr[j], arr[j + 1] = arr[j + 1], arr[j]
    return arr

# Test cases
print("Test case 1:")
arr = [64, 34, 25, 12, 22, 11, 90]
print("Original array:", arr)
print("Sorted array:", bubble_sort(arr))

print("\nTest case 2:")
arr = [5, 2, 8, 1, 9]
print("Original array:", arr)
print("Sorted array:", bubble_sort(arr))

print("\nTest case 3:")
arr = [10, 7, 8, 9, 1, 5]
print("Original array:", arr)
print("Sorted array:", bubble_sort(arr))
